{
  "buildCommand": "turbo run build --filter=web...",
  "outputDirectory": "apps/web/.next",
  "framework": "nextjs",
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    }
  ]
}
